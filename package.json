{
  "scripts": {
    "start": "babel-node build/server.js",
    "dev": "nodemon --exec babel-node src/server.js",
    "build": "yarn clean && babel -d ./build ./src --ignore '**/*.spec.js'",
    "clean": "rm -rf build && mkdir build",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix --ext .js .",
    "migrate": "yarn sequelize db:migrate",
    "migrate:undo": "yarn sequelize db:migrate:undo:all",
    "migrate:undo:prev": "yarn sequelize db:migrate:undo",
    "gen:seed": "yarn sequelize seed:generate",
    "seed": "yarn sequelize db:seed:all",
    "seed:undo": "yarn sequelize db:seed:undo:all",
    "reseed:db": "yarn sequelize db:seed:undo && yarn sequelize db:seed:all",
    "generate:model": "yarn sequelize model:generate",
    "db:reload": "yarn sequelize db:seed:undo && sequelize db:migrate:undo:all && yarn sequelize db:migrate && yarn sequelize db:seed:all"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "faker": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.17",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0",
    "uuid": "^8.3.2"
  },
  "name": "volunteerspark",
  "version": "1.0.0",
  "main": "server.js",
  "repository": "https://github.com/obayomi96/volunteerspark.git",
  "author": "Martins Obayomi <obayomimartins96@gmail.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.12.13",
    "@babel/preset-env": "^7.11.0",
    "babel-loader": "^8.2.2",
    "@babel/register": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "debug": "^4.3.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  }
}
